            data-car_page_type="{{item.attr.car_page_type}}"
            data-redirect_type="{{item.attr.redirect_type}}"
            data-redirect_target="{{item.attr.redirect_target}}"
            data-wx_app_path="{{item.attr.wx_app_path}}"
            data-wx_app_id="{{item.attr.wx_app_id}}"
            bindtap="cardTap">
            <block  wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
                <template is="wxParse1" data="{{item}}"/>
