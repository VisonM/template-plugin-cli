 <!--基础元素-->
<template name="wxParseVideo">
    <!--增加video标签支持，并循环添加-->
    <view class="{{item.classStr}} wxParse-{{item.tag}}" style="{{item.styleStr}}">
      <!-- video临时改用腾讯视频插件-v49 -->
      <news-video
        tencentVid="{{item.tencent_vid}}"
        vid="{{item.attr.src}}"
        cover="{{item.attr.poster}}"
        videoClassStr="{{item.classStr}}"
        height="422rpx"
      />
    </view>
</template>

<template name="wxParseImg">
    <image
        lazy-load="true"
        class="{{item.classStr}} wxParse-{{item.tag}} ui-image-default"
        data-from="{{item.from}}"
        data-src="{{item.attr.src}}"
        data-idx="{{item.imgIndex}}"
        src="{{item.attr.src}}"
        mode="widthFix"
        bindload="wxParseImgLoad"
        bindtap="{{item.redirect_type ? '' : 'wxParseImgTap'}}"
        style="width:{{item.width}}px;"
    />
</template>

<template name="WxEmojiView">
  <span class="WxEmojiView wxParse-inline" style="{{item.styleStr}}">
    <block wx:for="{{item.textArray}}" wx:key="">
      <block class="{{item.text == '\\n' ? 'wxParse-hide':''}}" wx:if="{{item.node == 'text'}}">{{item.text}}</block>
      <block wx:elif="{{item.node == 'element'}}">
        <image class="wxEmoji" src="{{item.baseSrc}}{{item.text}}" />
      </block>
    </block>
  </span>
</template>

<!--入口模版-->

<template name="wxParse">
    <block wx:for="{{wxParseData}}" wx:key="">
        <template is="wxParse0" data="{{item}}"/>
    </block>
</template>


<!--循环模版-->
<template name="wxParse0">
    <!--判断是否是标签节点-->
    <block wx:if="{{item.node == 'element'}}">
        <block wx:if="{{item.tag == 'button'}}">
            <button type="default" size="mini" >
                <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
                    <template is="wxParse1" data="{{item}}"/>
                </block>
             </button>
        </block>
        <!--li类型-->
        <block wx:elif="{{item.tag == 'li'}}">
            <view class="{{item.classStr}} wxParse-li" style="{{item.styleStr}}">
                <view class="{{item.classStr}} wxParse-li-inner">
                    <view class="{{item.classStr}} wxParse-li-text">
                        <view class="{{item.classStr}} wxParse-li-circle"></view>
                    </view>
                    <view class="{{item.classStr}} wxParse-li-text">
                        <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
                            <template is="wxParse1" data="{{item}}"/>
                        </block>
                    </view>
                </view>
            </view>
        </block>

        <!--video类型-->
        <block wx:elif="{{item.tag == 'video'}}">
            <template is="wxParseVideo" data="{{item}}"/>
        </block>

        <!--img类型-->
        <block wx:elif="{{item.tag == 'img'}}">
            <template is="wxParseImg" data="{{item}}"/>
        </block>
        <!--其他块级标签-->
        <block wx:elif="{{item.tagType == 'block'}}">
            <view class="{{item.classStr}} wxParse-{{item.tag}}" style="{{item.styleStr}}" data-redirect_type="{{item.attr.redirect_type}}"  data-redirect_target="{{item.attr.redirect_target}}" catchtap="blockTap">
                <block  wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
                    <template is="wxParse1" data="{{item, redirect_type:item.attr.redirect_type}}"/>
                </block>
            </view>
        </block>
                <!--a类型-->
        <block wx:elif="{{item.tag == 'a'}}">
          <view bindtap="wxParseTagATap" class="wxParse-inline 1212  {{item.classStr}} wxParse-{{item.tag}}" data-src="{{item.attr.href}}"  style="{{item.styleStr}}">
            <!-- 跳转小程序 文字 -->
            <view 
                class="custom-navigator__wrap" 
                wx:if="{{item.classStr=='mp-miniprogram weapp_text_link'}}" 
                data-redirect_type="{{item.attr.redirect_type}}"  
                data-redirect_target="{{item.attr.redirect_target}}" 
                data-wx_app_path="{{item.attr.wx_app_path}}" 
                data-wx_app_id="{{item.attr.wx_app_id}}"
                catchtap="cardTap"
            >
                <custom-service-btn type="{{item.attr.redirect_type}}" target="{{item.attr.redirect_target}}">
                    <!-- <navigator target="{{item.attr['wx_app_id'] != 'wxd89b0def414d9163' && item.attr['wx_app_id'] != 'wxa1219d243fca70f7'?'miniProgram':''}}" open-type="navigate" app-id="{{item.attr['wx_app_id']}}" path="{{item.attr['wx_app_path']}}" > -->
                      <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
                        <template is="wxParse1" data="{{item}}" />
                      </block>
                    <!-- </navigator> -->
                </custom-service-btn>
            </view>
            <!-- 跳转小程序 图片 -->
            <view 
                wx:elif="{{item.classStr=='mp-miniprogram weapp_image_link'}}" 
                data-redirect_type="{{item.attr.redirect_type}}"  
                data-redirect_target="{{item.attr.redirect_target}}" 
                data-wx_app_path="{{item.attr.wx_app_path}}" 
                data-wx_app_id="{{item.attr.wx_app_id}}"
                catchtap="cardTap"
            >
                <custom-service-btn type="{{item.attr.redirect_type}}" target="{{item.attr.redirect_target}}">
                    <!-- <navigator target="{{item.attr['wx_app_id'] != 'wxd89b0def414d9163' && item.attr['wx_app_id'] != 'wxa1219d243fca70f7'?'miniProgram':''}}" open-type="navigate" app-id="{{item.attr['wx_app_id']}}" path="{{item.attr['wx_app_path']}}"> -->
                        <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
                            <template is="wxParse1" data="{{item}}" />
                        </block>
                    <!-- </navigator> -->
                    <view class="image-logo" ></view>
                </custom-service-btn>
            </view>
 
            <view wx:else>
                <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
                    <template is="wxParse1" data="{{item}}" />
                </block>
            </view>
          </view>
        </block>

